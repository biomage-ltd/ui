
{
  "presets": [
    [
      "next/babel",
      {
        "preset-env":  {
          "useBuiltIns": "usage",
          "corejs": 2,
          "targets": {
            "ie": 11
          }
        }
      }
    ]
  ],
  "plugins": [
    ["lodash"],
    ["@babel/plugin-transform-runtime"]
  ],
  "env": {
    "test": {
      "presets": [
        [
          "next/babel",
          {
            "transform-runtime": {
              "corejs": false
            }
          }
        ]
      ],
      "plugins": [
        "babel-plugin-dynamic-import-node"
      ]
    }
  }
}